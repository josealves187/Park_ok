<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragments.AttendanceFragment">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/mt_toolbar_attendance"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="1dp"
        app:title="@string/title_toolbar_calls"
        app:titleTextColor="@color/color_toolbar_menu"
        app:titleTextAppearance="@style/MaterialToolbarMenuStyleTheme"/>

   <androidx.recyclerview.widget.RecyclerView
       android:id="@+id/rv_calls"
       android:layout_width="0dp"
       android:layout_height="match_parent"
       android:layout_marginTop="60dp"
       android:layout_marginBottom="20dp"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintTop_toBottomOf="@id/mt_toolbar_attendance">

   </androidx.recyclerview.widget.RecyclerView>


</androidx.constraintlayout.widget.ConstraintLayout>